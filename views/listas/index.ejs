<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Listas</title>
</head>

<body>
    <h1>Listas</h1>


    <ul>
        <% lista.forEach( function (listas) { %>
            <li>
                <%=listas.id%>
                <%=listas.nome%>
                <%=listas.preco%>
            </li>
            <% }) %>
    </ul>

    
</body>

</html>